{
	"version": "2.0.0",
	"tasks": [
        {
            "label": "Android: List Devices",
            "type": "shell",
            "windows": {
                "command": "powershell",
                "args": ["-ExecutionPolicy", "Bypass", "-File", "${workspaceFolder}/scripts/list-android-devices.ps1"]
            },
            "linux": {
                "command": "${workspaceFolder}/scripts/list-android-devices.sh"
            },
            "osx": {
                "command": "${workspaceFolder}/scripts/list-android-devices.sh"
            },
            "group": "build",
            "presentation": {
                "echo": true,
                "reveal": "always",
                "focus": true,
                "panel": "new",
                "showReuseMessage": false,
                "clear": true
            },
            "problemMatcher": []
        },
        {
            "label": "compile-cube-shaders",
            "command": "dotnet",
            "type": "process",
            "args": [
                "msbuild",
				"examples/cube/cube.csproj",
                "-t:CompileShaders"
            ],
            "problemMatcher": "$msCompile"
        },
		{
            "label": "compile-dyntex-shaders",
